<%- include('includes/head') %> <%- include('includes/nav') %>

<div class="container">
  <div class="row">
    <div class="col-lg-2"></div>

    <div class="col-lg-8 my-3">
      <h1 class="display-1 text-center">Contato</h1>
      <p class="text-center lead">Crie ou edite seus contatos abaixo.</p>


      <form class="form-cadastro" action="/login/register" method="POST">
        <input type="hidden" name="_csrf" value=<%= csrfToken %>>
        <div class="form-group">
          <label>Nome</label>
          <input type="text" class="form-control" name="Nome">
        </div>
        <div class="form-group">
          <label>Sobrenome</label>
          <input type="text" class="form-control" name="Sobrenome">
        </div>
        <div class="form-group">
          <label>Email</label>
          <input type="email" class="form-control" name="Email">
        </div>
        <div class="form-group">
          <label>Telefone</label>
          <input type="tel" class="form-control" name="Telefone">
        </div>
        <button type="Salvar" class="btn btn-primary">Criar minha conta</button>
      </form>
      
      <%- include('includes/message')%> 

     
      </div>
    </div class="col-lg-2"></div>
    </div>
</div>
<%- include('includes/footer') %>
